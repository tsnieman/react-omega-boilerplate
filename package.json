{
  "name": "react-omega-boilerplate",
  "version": "0.0.0",
  "description": "Personal React boilerplate for Tyler Nieman (tsnieman).",
  "scripts": {
    "clean": "rm -rf ./dist/",
    "build:dev": "npm run clean",
    "build:prod": "npm run clean && webpack --config webpack.config.js -p",
    "start": "npm run build:dev && NODE_ENV=development node ./devServer.js",
    "test": "NODE_ENV=test BABEL_DISABLE_CACHE=1 jest --no-cache",
    "test:watch": "npm run test -- --watch"
  },
  "homepage": "https://github.com/tsnieman/react-omega-boilerplate",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "humps": "^1.1.0",
    "lodash": "^4.14.1",
    "normalizr": "^2.2.1",
    "pluralize": "^3.0.0",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.0",
    "react-redux": "^4.4.5",
    "recompose": "^0.20.2",
    "redux": "^3.5.2",
    "shortid": "^2.2.6",
    "webfontloader": "^1.6.26"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^14.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-webpack-loaders": "^0.7.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.9.0",
    "jest-cli": "^14.0.1",
    "jsdom": "^9.4.1",
    "nock": "^8.0.0",
    "node-libs-browser": "^1.0.0",
    "postcss-advanced-variables": "^1.2.2",
    "postcss-color-function": "^2.0.1",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-hexrgba": "^0.2.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-mixins": "^5.0.0",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "^15.3.1",
    "react-css-modules": "^3.7.9",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redbox-react": "^1.2.10",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-saga": "^0.11.0",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.12.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "lodash"
    ],
    "modulePaths": [
      "/src"
    ]
  }
}
